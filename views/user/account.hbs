<div class="row">
  <div class="col-sm-6 col-md-6 col-md-offset-3 col-sm-offset-3">
    <div id="success" class="alert alert-success {{#if noMessage}}hidden{{/if}}">
      {{successMsg}}
    </div>
  </div>
</div>
<div class="row">
        <div class="col-sm-6">
            <div class="well">
                <a href="/imageupload" class="btn btn-primary">Upload your picture</a>
                <h3><span class="fa fa-user"></span> Local</h3>
                {{# if user}}
                    <p>
                        <strong>id</strong>: {{this.user._id}}<br>
                        <strong>username</strong>: {{this.user.username}}<br>
                        <strong>email</strong>: {{this.user.email}}<br>
                        <strong>Address</strong>: {{this.user.Address}}<br>
                        <strong>Image Path</strong>: {{this.user.imgPath}}
                        <img src="{{this.user.imgPath}}" alt="no img" class="img-thumbnail" style="width:100px">
                    </p>
                {{/if}}
            </div>
        </div>

    </div>

